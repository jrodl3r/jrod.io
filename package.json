{
  "name": "jrod-v4",
  "version": "4.0.0",
  "main": "dist/jrod-v4/server/main.js",
  "scripts": {
    "ng": "ng",
    "start": "node dist/jrod-v4/server/main.js",
    "start:dev": "ng serve --host 0.0.0.0 --port 4100 --disable-host-check",
    "start:prod": "ng serve --host 0.0.0.0 --port 4300 --disable-host-check --configuration production",
    "start:ssr": "ng run jrod-v4:serve-ssr",
    "serve:ssr": "node dist/jrod-v4/server/main.js",
    "build": "ng build --configuration production",
    "build:ssr": "npm run build && ng run jrod-v4:server",
    "release": "npm run build:ssr && gcloud app deploy --project=jrod-new",
    "test": "ng test",
    "prerender": "ng run jrod-v4:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/cdk": "^12.2.13",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/material": "^12.2.13",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/platform-server": "~13.3.0",
    "@angular/router": "~13.3.0",
    "@emailjs/browser": "^3.4.0",
    "@nguniversal/express-engine": "^13.1.0",
    "express": "^4.15.2",
    "hammerjs": "^2.0.8",
    "ngx-google-analytics": "^13.0.1",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.0",
    "@angular/cli": "~13.3.0",
    "@angular/compiler-cli": "~13.3.0",
    "@nguniversal/builders": "^13.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.6.2"
  }
}
